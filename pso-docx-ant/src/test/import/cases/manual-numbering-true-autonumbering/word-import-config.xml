<?xml version="1.0"?>

<config>
	<lists>
	  <convert-manual-numbering select="true">
		<value match="Schedule&#160;[0-9]+">
	      <autonumbering/>
	    </value>
		<value match="Part&#160;[0-9]+">
	      <autonumbering/>
	    </value>
		<value match="\s*[0-9]+\s">
	      <autonumbering/>
	    </value>
		<value match="\s*\([0-9]+[A-Z]+\)">
	      <autonumbering/>
	    </value>
		<value match="\s*\([a-z]+\)">
	      <autonumbering/>
	    </value>
	  </convert-manual-numbering> 
  </lists>
  <styles>
	<default>
      <paragraphStyles value="para" />
    </default>
  </styles>
</config>
