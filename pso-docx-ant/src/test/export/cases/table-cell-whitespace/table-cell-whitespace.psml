	<section id="title">
		<fragment id="title">
			<heading level="1">Cell whitespace</heading>
		</fragment>
	</section>
	<section id="1">
		<fragment id="1">
      <para>Table with no colspans or rowspans:</para>
      <table>
        <row>
          <cell>Some text 1</cell>
          <cell>
            <para>Some text 2</para>          
          </cell>
        </row>
        <row>
          <cell>Some <bold>bold</bold> <italic>text</italic> 3</cell>
          <cell><preformat>pre</preformat>Some <bold>bold</bold> <italic>text</italic> 4</cell>
        </row>
        <row>
          <cell>
            <preformat>pre</preformat>Some <bold>bold</bold> <italic>text</italic><preformat>pre</preformat> and <underline>more</underline> 4</cell>
          <cell> <bold> </bold> <italic> </italic> </cell>
        </row>
        <row>
          <cell>  </cell>
          <cell><image src="ico-url.png" width="20" height="20"/></cell>
        </row>
      </table>   
      <para>End</para>
		</fragment>
	</section>
